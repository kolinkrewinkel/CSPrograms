
-- Example code from author's site.

local JSON = (dofile "JSON.lua") -- one-time load of the routines
local response = http.get("https://api.github.com/repos/kolinkrewinkel/CSPrograms/commits")

local lua_value = JSON:decode(response.readAll()) -- decode example
local monitor = peripheral.wrap("top")
monitor.clear()
monitor.setCursorPos(1,1)
monitor.write(lua_value)
